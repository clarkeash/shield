{
    "name": "clarkeash/shield",
    "description": "A laravel middleware to shield against unverified webhooks from services",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "orchestra/testbench": "~3.5@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ashley Clarke",
            "email": "me@ashleyclarke.me"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {},
    "autoload": {
        "psr-4": {
            "Clarkeash\\Shield\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clarkeash\\Shield\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Clarkeash\\Shield\\Providers\\ShieldServiceProvider"
            ]
        }
    }
}
